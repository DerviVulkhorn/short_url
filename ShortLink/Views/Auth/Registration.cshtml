@{
}
<form method="post" asp-action="Register" asp-controller="Auth">
    <div class="mb-3">
        <label asp-for="email" for="exampleInputEmail1" class="form-label">Email address</label>
        <input asp-for="email" type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
        <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
    </div>
    <div class="mb-3">
        <label asp-for="password" for="exampleInputPassword1" class="form-label">Password</label>
        <input asp-for="password" type="password" class="form-control" id="exampleInputPassword1">
    </div>
    <div class="mb-3 form-check">
        <input type="checkbox" class="form-check-input" id="exampleCheck1">
        <label class="form-check-label" for="exampleCheck1">Check me out</label>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>